<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns:fx="http://javafx.com/fxml/1" fx:id="hBox" spacing="10.0" xmlns="http://javafx.com/javafx/8"
      fx:controller="io.github.funkynoodles.FXMLExpenseOverTimeCategoryFieldController">
    <children>
        <Label fx:id="categoryLabel" prefHeight="31.0" prefWidth="120.0" text="Label"/>
        <CheckBox fx:id="displayCheckBox" mnemonicParsing="false" onAction="#handleDisplayCheckBoxToggle"
                  prefHeight="31.0" text="Display"/>
        <Button fx:id="removeButton" mnemonicParsing="false" onAction="#handleRemoveButton" text="Remove"/>
    </children>
</HBox>
