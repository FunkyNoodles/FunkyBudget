<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" spacing="10.0" xmlns="http://javafx.com/javafx/8"
      fx:controller="io.github.funkynoodles.FXMLReadGnuCashTransCSVController">
    <children>
        <HBox spacing="10.0">
            <children>
                <Label prefHeight="31.0" text="File:"/>
                <TextField fx:id="filePath"/>
                <Button fx:id="browseButton" mnemonicParsing="false" onAction="#handleBrowseButton" text="..."/>
            </children>
        </HBox>
        <HBox alignment="TOP_CENTER" spacing="10.0">
            <children>
                <Button fx:id="importButton" mnemonicParsing="false" onAction="#handleImportButton" text="Import"/>
                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#handleCancelButton" text="Cancel"/>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
